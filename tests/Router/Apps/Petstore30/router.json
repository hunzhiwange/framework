{
    "base_paths": {
        "*": {
            "middlewares": []
        },
        "\/^\\\/foo\\\/(\\S+)world\\\/$\/": {
            "middlewares": []
        },
        "\/^\\\/api\\\/test\\\/$\/": {
            "middlewares": []
        }
    },
    "group_paths": {
        "\/api\/v1": {
            "middlewares": []
        },
        "\/api\/v2": {
            "middlewares": []
        },
        "\/web\/v1": {
            "middlewares": []
        },
        "\/web\/v2": {
            "middlewares": []
        }
    },
    "groups": [
        "\/pet",
        "\/store",
        "\/user"
    ],
    "routers": {
        "get": {
            "p": {
                "\/pet": {
                    "\/api\/v1\/petLeevelForApi\/{petId:[A-Za-z]+}\/": {
                        "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Api@petLeevelForApi",
                        "var": [
                            "petId"
                        ]
                    },
                    "\/api\/v2\/petLeevel\/{petId:[A-Za-z]+}\/": {
                        "scheme": "https",
                        "domain": "{subdomain:[A-Za-z]+}-vip.{domain}.queryphp.cn",
                        "params": {
                            "args1": "hello",
                            "args2": "world"
                        },
                        "bind": "\\PetLeevel\\show",
                        "middlewares": [],
                        "domain_regex": "\/^([A-Za-z]+)\\-vip\\.(\\S+)\\.queryphp\\.cn$\/",
                        "domain_var": [
                            "subdomain",
                            "domain"
                        ],
                        "var": [
                            "petId"
                        ]
                    },
                    "\/pet\/{petId}\/": {
                        "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Pet@getPetById",
                        "var": [
                            "petId"
                        ]
                    },
                    "\/web\/v1\/petLeevelForWeb\/{petId:[A-Za-z]+}\/": {
                        "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Web@petLeevelForWeb",
                        "var": [
                            "petId"
                        ]
                    },
                    "regex": [
                        "~^(?|\/api\/v1\/petLeevelForApi\/([A-Za-z]+)\/|\/api\/v2\/petLeevel\/([A-Za-z]+)\/()|\/pet\/(\\S+)\/()()|\/web\/v1\/petLeevelForWeb\/([A-Za-z]+)\/()()())$~x"
                    ],
                    "map": [
                        {
                            "2": "\/api\/v1\/petLeevelForApi\/{petId:[A-Za-z]+}\/",
                            "3": "\/api\/v2\/petLeevel\/{petId:[A-Za-z]+}\/",
                            "4": "\/pet\/{petId}\/",
                            "5": "\/web\/v1\/petLeevelForWeb\/{petId:[A-Za-z]+}\/"
                        }
                    ]
                }
            },
            "static": {
                "\/pet\/findByTags\/": {
                    "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Pet@findByTags"
                },
                "\/store\/": {
                    "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Store@getInventory"
                },
                "\/user\/login\/": {
                    "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\User@loginUser"
                },
                "\/user\/logout\/": {
                    "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\User@logoutUser"
                }
            },
            "s": {
                "\/store": {
                    "\/store\/order\/{orderId}\/": {
                        "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Store@getOrderById",
                        "var": [
                            "orderId"
                        ]
                    },
                    "regex": [
                        "~^(?|\/store\/order\/(\\S+)\/)$~x"
                    ],
                    "map": [
                        {
                            "2": "\/store\/order\/{orderId}\/"
                        }
                    ]
                }
            },
            "u": {
                "\/user": {
                    "\/user\/{username}\/": {
                        "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\User@getUserByName",
                        "var": [
                            "username"
                        ]
                    },
                    "regex": [
                        "~^(?|\/user\/(\\S+)\/)$~x"
                    ],
                    "map": [
                        {
                            "2": "\/user\/{username}\/"
                        }
                    ]
                }
            }
        },
        "post": {
            "static": {
                "\/pet\/": {
                    "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Pet@addPet"
                },
                "\/store\/order\/": {
                    "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Store@placeOrder"
                },
                "\/user\/": {
                    "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\User@createUser"
                },
                "\/user\/createWithArray\/": {
                    "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\User@createUsersWithListInput"
                }
            },
            "p": {
                "\/pet": {
                    "\/pet\/{petId}\/": {
                        "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Pet@updatePetWithForm",
                        "var": [
                            "petId"
                        ]
                    },
                    "\/pet\/{petId}\/uploadImage\/": {
                        "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Pet@uploadFile",
                        "var": [
                            "petId"
                        ]
                    },
                    "regex": [
                        "~^(?|\/pet\/(\\S+)\/|\/pet\/(\\S+)\/uploadImage\/())$~x"
                    ],
                    "map": [
                        {
                            "2": "\/pet\/{petId}\/",
                            "3": "\/pet\/{petId}\/uploadImage\/"
                        }
                    ]
                }
            }
        },
        "delete": {
            "p": {
                "\/pet": {
                    "\/pet\/{petId}\/": {
                        "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Pet@deletePet",
                        "var": [
                            "petId"
                        ]
                    },
                    "regex": [
                        "~^(?|\/pet\/(\\S+)\/)$~x"
                    ],
                    "map": [
                        {
                            "2": "\/pet\/{petId}\/"
                        }
                    ]
                }
            },
            "s": {
                "\/store": {
                    "\/store\/order\/{orderId}\/": {
                        "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\Store@deleteOrder",
                        "var": [
                            "orderId"
                        ]
                    },
                    "regex": [
                        "~^(?|\/store\/order\/(\\S+)\/)$~x"
                    ],
                    "map": [
                        {
                            "2": "\/store\/order\/{orderId}\/"
                        }
                    ]
                }
            },
            "u": {
                "\/user": {
                    "\/user\/{username}\/": {
                        "bind": "\\Tests\\Router\\Apps\\Petstore30\\Controllers\\User@deleteUser",
                        "var": [
                            "username"
                        ]
                    },
                    "regex": [
                        "~^(?|\/user\/(\\S+)\/)$~x"
                    ],
                    "map": [
                        {
                            "2": "\/user\/{username}\/"
                        }
                    ]
                }
            }
        }
    }
}

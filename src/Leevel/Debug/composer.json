{
    "name": "leevel/debug",
    "description": "The Leevel Debug Package.",
    "license": "MIT",
    "homepage": "http://queryphp.com",
    "support": {
        "issues": "https://github.com/hunzhiwange/framework/issues",
        "source": "https://github.com/hunzhiwange/framework"
    },
    "authors": [{
        "name": "Xiangmin Liu",
        "email": "635750556@qq.com"
    }],
    "require": {
        "php": "^8.0",
        "leevel/di": "1.0.*",
        "leevel/support": "1.0.*",
        "maximebf/debugbar": "~1.16.3",
        "spiral/dumper": "^1.1.3",
        "symfony/var-dumper": "~5.1",
        "symfony/http-foundation": "~5.1"
    },
    "autoload": {
        "psr-4": {
            "Leevel\\Debug\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "leevel": {
            "providers": [
                "Leevel\\Debug\\Provider\\Register"
            ],
            "commands": [
                "Leevel\\Debug\\Console"
            ]
        }
    },
    "minimum-stability": "dev"
}

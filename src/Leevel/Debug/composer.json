{
    "name": "leevel/debug",
    "description": "The Leevel Debug Package.",
    "license": "MIT",
    "homepage": "http://queryphp.com",
    "support": {
        "issues": "https://github.com/hunzhiwange/framework/issues",
        "source": "https://github.com/hunzhiwange/framework"
    },
    "authors": [{
        "name": "Xiangmin Liu",
        "email": "635750556@qq.com"
    }],
    "require": {
        "php": ">=8.1",
        "leevel/di": "^1.1",
        "leevel/support": "^1.1",
        "leevel/kernel": "^1.1",
        "leevel/database": "^1.1",
        "leevel/event": "^1.1",
        "leevel/log": "^1.1",
        "leevel/http": "^1.1",
        "leevel/session": "^1.1",
        "maximebf/debugbar": "^1.16.4",
        "symfony/var-dumper": "^6.2",
        "symfony/http-foundation": "^6.2"
    },
    "suggest": {
        "leevel/console": "Required to use the make commands (^1.1).",
        "leevel/filesystem": "Required to use the make commands (^1.1).",
        "leevel/kernel": "Required to use the make commands (^1.1)."
    },
    "autoload": {
        "psr-4": {
            "Leevel\\Debug\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "leevel": {
            "providers": [
                "Leevel\\Debug\\Provider\\Register"
            ],
            "commands": [
                "Leevel\\Debug\\Console"
            ]
        }
    },
    "minimum-stability": "dev"
}

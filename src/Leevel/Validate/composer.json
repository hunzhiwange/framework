{
    "name": "leevel/validate",
    "description": "The Leevel Validate Package.",
    "license": "MIT",
    "homepage": "http://queryphp.com",
    "support": {
        "issues": "https://github.com/hunzhiwange/framework/issues",
        "source": "https://github.com/hunzhiwange/framework"
    },
    "authors": [{
        "name": "Xiangmin Liu",
        "email": "635750556@qq.com"
    }],
    "require": {
        "php": ">=8.1",
        "leevel/i18n": "^1.1",
        "leevel/support": "^1.1",
        "leevel/di": "^1.1",
        "leevel/flow": "^1.1"
    },
    "suggest": {
        "leevel/database": "Required to use database unique rule (^1.1)."
    },
    "autoload": {
        "psr-4": {
            "Leevel\\Validate\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "leevel": {
            "providers": [
                "Leevel\\Validate\\Provider\\Register"
            ],
            "i18n-paths": {
                "validate": [
                    "vendor/hunzhiwange/framework/src/Leevel/Validate"
                ]
            }
        }
    },
    "minimum-stability": "dev"
}

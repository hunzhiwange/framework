{
    "name": "leevel/router",
    "description": "The Leevel Router Package.",
    "license": "MIT",
    "homepage": "http://queryphp.com",
    "support": {
        "issues": "https://github.com/hunzhiwange/framework/issues",
        "source": "https://github.com/hunzhiwange/framework"
    },
    "authors": [{
        "name": "Xiangmin Liu",
        "email": "635750556@qq.com"
    }],
    "require": {
        "php": ">=8.1",
        "leevel/di": "^1.1",
        "leevel/support": "^1.1",
        "leevel/http": "^1.1",
        "leevel/kernel": "^1.1",
        "leevel/session": "^1.1",
        "leevel/view": "^1.1"
    },
    "suggest": {
        "leevel/filesystem": "Required to use the make commands (^1.1).",
        "leevel/console": "Required to use the make commands (^1.1)."
    },
    "autoload": {
        "psr-4": {
            "Leevel\\Router\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "leevel": {
            "providers": [
                "Leevel\\Router\\Provider\\Register"
            ],
            "commands": "Leevel\\Router\\Console"
        }
    },
    "minimum-stability": "dev"
}

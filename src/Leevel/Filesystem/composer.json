{
    "name": "leevel/filesystem",
    "description": "The Leevel Filesystem Package.",
    "license": "MIT",
    "homepage": "http://queryphp.com",
    "support": {
        "issues": "https://github.com/hunzhiwange/framework/issues",
        "source": "https://github.com/hunzhiwange/framework"
    },
    "authors": [{
        "name": "Xiangmin Liu",
        "email": "635750556@qq.com"
    }],
    "require": {
        "php": ">=8.1",
        "leevel/support": "^1.1",
        "league/flysystem": "^3.12.3",
        "league/flysystem-ftp": "^3.10.3",
        "symfony/filesystem": "^6.2"
    },
    "suggest": {
        "league/flysystem-sftp-v3": "Sftp adapter support. (^3.10.3)",
        "league/flysystem-ziparchive": "ZipArchive adapter support. (^3.12.0)"
    },
    "autoload": {
        "psr-4": {
            "Leevel\\Filesystem\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "leevel": {
            "providers": [
                "Leevel\\Filesystem\\Provider\\Register"
            ]
        }
    },
    "minimum-stability": "dev"
}

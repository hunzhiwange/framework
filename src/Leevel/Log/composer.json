{
    "name": "leevel/log",
    "description": "The Leevel Log Package.",
    "license": "MIT",
    "homepage": "http://queryphp.com",
    "support": {
        "issues": "https://github.com/hunzhiwange/framework/issues",
        "source": "https://github.com/hunzhiwange/framework"
    },
    "authors": [{
        "name": "Xiangmin Liu",
        "email": "635750556@qq.com"
    }],
    "require": {
        "php": ">=8.1",
        "leevel/support": "^1.1",
        "leevel/event": "^1.1",
        "monolog/monolog": "^3.3.1",
        "psr/log": "^3.0.0"
    },
    "suggest": {
        "leevel/http": "Required to use log middleware (^1.1).",
        "leevel/filesystem": "Required to use the make commands (^1.1).",
        "leevel/console": "Required to use the make commands (^1.1)."
    },
    "autoload": {
        "psr-4": {
            "Leevel\\Log\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "leevel": {
            "providers": [
                "Leevel\\Log\\Provider\\Register"
            ],
            "commands": "Leevel\\Log\\Console"
        }
    },
    "minimum-stability": "dev"
}

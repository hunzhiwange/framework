        "queryyetsimple/di": "self.version",
        "queryyetsimple/encryption": "self.version",
        "queryyetsimple/event": "self.version",
        "queryyetsimple/filesystem": "self.version",
        "queryyetsimple/flow": "self.version",
        "queryyetsimple/http": "self.version",
        "queryyetsimple/i18n": "self.version",
        "queryyetsimple/log": "self.version",
        "queryyetsimple/mail": "self.version",
        "queryyetsimple/manager": "self.version",
        "queryyetsimple/mvc": "self.version",
        "queryyetsimple/option": "self.version",
        "queryyetsimple/page": "self.version",
        "queryyetsimple/pipeline": "self.version",
        "queryyetsimple/psr4": "self.version",
        "queryyetsimple/queue": "self.version",
        "queryyetsimple/router": "self.version",
        "queryyetsimple/seccode": "self.version",
        "queryyetsimple/session": "self.version",
        "queryyetsimple/stack": "self.version",
        "queryyetsimple/support": "self.version",
        "queryyetsimple/swoole": "self.version",
        "queryyetsimple/throttler": "self.version",
        "queryyetsimple/tree": "self.version",
        "queryyetsimple/validate": "self.version",
        "queryyetsimple/view": "self.version"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "phpunit/phpunit": "~7.0"
    },
    "suggest": {
        "ext-swoole": "Required to use swoole.",
        "ext-v8js": "Required to use php v8js.",
        "league/flysystem-sftp": "Sftp adapter support.",
        "league/flysystem-ziparchive": "ZipArchive adapter support."
    },
    "autoload": {
        "files": [
            "src\\Queryyetsimple\\Bootstrap\\function.php"
        ],
        "psr-4": {
            "Queryyetsimple\\": "src\\Queryyetsimple"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"

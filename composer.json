        "coderkungfu/php-queue": "^0.9.1",
        "clio/clio": "@stable",
        "robmorgan/phinx": "^0.8.0",
        "vlucas/phpdotenv": "~2.2",
        "nesbot/carbon": "~1.20",
        "league/flysystem": "^1.0",
        "monolog/monolog": "^1.23",
        "swiftmailer/swiftmailer": "6.0.2",
        "nunomaduro/collision": "dev-master"
    },
    "replace": {
        "queryyetsimple/aop": "self.version",
        "queryyetsimple/auth": "self.version",
        "queryyetsimple/cache": "self.version",
        "queryyetsimple/collection": "self.version",
        "queryyetsimple/console": "self.version",
        "queryyetsimple/cookie": "self.version",

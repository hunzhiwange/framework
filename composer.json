{
    "name": "hunzhiwange/framework",
    "description": "The QueryPHP Framework.",
    "keywords": ["framework", "queryphp"],
    "license": "MIT",
    "homepage": "http://www.queryphp.com",
    "support": {
        "issues": "https://github.com/hunzhiwange/framework/issues",
        "source": "https://github.com/hunzhiwange/framework"
    },
    "authors": [
        {
            "name": "Xiangmin Liu",
            "email": "635750556@qq.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "symfony/console": "3.0.*",
        "symfony/var-dumper": "2.8.*|3.0.*",
        "predis/predis": "^1.1",
        "coderkungfu/php-queue": "^0.9.1",
        "robmorgan/phinx": "^0.8.0",
        "vlucas/phpdotenv": "~2.2",
        "nesbot/carbon": "~1.20",
        "league/flysystem": "^1.0"
    },
     "replace": {
        "queryyetsimple/assert": "self.version",
        "queryyetsimple/auth": "self.version",
        "queryyetsimple/cache": "self.version",
        "queryyetsimple/collection": "self.version",
        "queryyetsimple/console": "self.version",
        "queryyetsimple/cookie": "self.version",
        "queryyetsimple/database": "self.version",
        "queryyetsimple/debug": "self.version",
        "queryyetsimple/encryption": "self.version",
        "queryyetsimple/event": "self.version",
        "queryyetsimple/filesystem": "self.version",
        "queryyetsimple/http": "self.version",
        "queryyetsimple/i18n": "self.version",
        "queryyetsimple/log": "self.version",
        "queryyetsimple/mail": "self.version",
        "queryyetsimple/mvc": "self.version",
        "queryyetsimple/observer": "self.version",
        "queryyetsimple/option": "self.version",
        "queryyetsimple/page": "self.version",
        "queryyetsimple/pipeline": "self.version",
        "queryyetsimple/queue": "self.version",
        "queryyetsimple/router": "self.version",
        "queryyetsimple/rss": "self.version",
        "queryyetsimple/safe": "self.version",
        "queryyetsimple/seccode": "self.version",
        "queryyetsimple/session": "self.version",
        "queryyetsimple/stack": "self.version",
        "queryyetsimple/support": "self.version",
        "queryyetsimple/throttler": "self.version",
        "queryyetsimple/validate": "self.version",
        "queryyetsimple/view": "self.version",
        "queryyetsimple/xml": "self.version"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "phpunit/phpunit": "~4.0"
    },
    "suggest": {
        "league/flysystem-sftp": "Sftp adapter support.",
        "league/flysystem-ziparchive": "ZipArchive adapter support."
    },
    "autoload": {
        "files": [
            "src\\queryyetsimple\\bootstrap\\function.php"
        ],
        "psr-4": {
            "queryyetsimple\\": "src\\queryyetsimple"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}